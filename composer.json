{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
           "_comment": "The countries geoJSON files as a library",
           "type": "package",
           "package": {
           "name": "mledoze/countries",
           "version": "dev-master",
           "type": "drupal-library",
           "dist": {
            "url": "https://github.com/mledoze/countries/archive/master.zip",
            "type": "zip"
        }
    }
}
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/bg_image_formatter": "^1.14",
        "drupal/block_background_style": "^1.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/bootstrap_simple_carousel": "^1.61",
        "drupal/captcha": "^1.2",
        "drupal/contact_block": "^1.5",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/ctools": "^3.7",
        "drupal/custom_formatters": "^3.0@beta",
        "drupal/devel": "^4.1",
        "drupal/diba_carousel": "^1.15",
        "drupal/ds": "^3.13",
        "drupal/ds_chains": "^1.1",
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/entity": "^1.2",
        "drupal/eu_cookie_compliance": "^1.14",
        "drupal/field_group": "^3.2",
        "drupal/fontawesome": "^2.19",
        "drupal/geofield": "^1.34",
        "drupal/google_analytics": "^3.1",
        "drupal/image_field_caption": "^1.1",
        "drupal/image_widget_crop": "^2.3",
        "drupal/leaflet": "^2.1",
        "drupal/leaflet_countries": "^1.1",
        "drupal/libraries": "^3.0@beta",
        "drupal/mailchimp": "^2.0",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.6",
        "drupal/pathauto": "^1.8",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/vefl": "^3.0",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_infinite_scroll": "^1.8",
        "drupal/views_parity_row": "^1.12",
        "mledoze/countries": "dev-master",
        "thinkshout/mailchimp-api-php": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
